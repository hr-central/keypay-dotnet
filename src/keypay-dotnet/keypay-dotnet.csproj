<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <RootNamespace>KeyPay</RootNamespace>
    <AssemblyName>KeyPay</AssemblyName>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageLicenseUrl>http://opensource.org/licenses/mit-license</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/keypay/keypay-dotnet</PackageProjectUrl>
    <PackageIconUrl>http://keypay.com.au/wp-content/uploads/2016/07/home-banner-icon.svg</PackageIconUrl>
    <PackageTags>keypay yourpayroll api payroll</PackageTags>
    <Description>A .NET library for the KeyPay API</Description>
    <PackageReleaseNotes>Add Bank Payments method to the PayRun endpoint</PackageReleaseNotes>
    <Copyright>Copyright 2018</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="RestSharp" Version="106.2.1" />
  </ItemGroup>

</Project>